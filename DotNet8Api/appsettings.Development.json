{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "GitHubSettings": {
    "BaseAddress": "https://api.github.com/",
    "AccessToken": "your-access-token",
    "UserAgent": "DotNet8-Examples"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\workspace\\pf-dso\\PlanForwardDSO\\logs\\log-.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj} {Properties}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "data source=DESKTOP-AI3KLJL\\SQLEXPRESS;initial catalog=PlanForwardDSO;persist security info=True;Integrated Security=True;TrustServerCertificate=True",
          "tableName": "Logs_PMSImport",
          "schemaName": "log",
          "autoCreateSqlTable": true
        }
      }
    ]
  }
}
